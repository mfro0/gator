	$(RM) radeon.$(DRIVERMANSUFFIX).html radeon.$(DRIVERMANSUFFIX)-html

all:: radeon.$(MANNEWSUFFIX)

radeon.$(MANNEWSUFFIX)::  radeon.$(MANSRCSUFFIX)
	$(RM) $@
	 	$(RAWCPP)  $(MANDEFS)  $(EXTRAMANDEFS) <radeon.$(MANSRCSUFFIX) | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@

cleandir::
	$(RM) radeon.$(MANNEWSUFFIX)

install.man:: radeon.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(DRIVERMANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DRIVERMANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(DRIVERMANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DRIVERMANDIR)); \
	fi
	$(INSTALL) -c $(INSTMANFLAGS) radeon.$(MANNEWSUFFIX) \
		$(DESTDIR)$(DRIVERMANDIR)/radeon.$(DRIVERMANSUFFIX)

