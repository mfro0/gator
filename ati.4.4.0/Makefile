install.sdk:: ati_drv.o
	@if [ -d $(DESTDIR)$(DRIVERSDKMODULEDIR)/drivers ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DRIVERSDKMODULEDIR)/drivers ]; then \
			(set -x; rm -f $(DESTDIR)$(DRIVERSDKMODULEDIR)/drivers); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DRIVERSDKMODULEDIR)/drivers); \
	fi
	$(INSTALL) -c ati_drv.o $(DESTDIR)$(DRIVERSDKMODULEDIR)/drivers

