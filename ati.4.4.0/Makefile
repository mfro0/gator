r128.$(DRIVERMANSUFFIX).html: r128.$(MANNEWSUFFIX) $(RMAN)
	$(RM) r128.$(DRIVERMANSUFFIX).html r128.$(DRIVERMANSUFFIX)-html
	$(RMAN) $(RMANOPTIONS) < r128.$(MANNEWSUFFIX) \
	  > r128.$(DRIVERMANSUFFIX)-html && $(MV) r128.$(DRIVERMANSUFFIX)-html $@

install.man:: r128.$(DRIVERMANSUFFIX).html
	@if [ -d $(DESTDIR)$(DOCHTMLDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DOCHTMLDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(DOCHTMLDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DOCHTMLDIR)); \
	fi
	@(SUF=`expr $(DRIVERMANSUFFIX) \: '\(.\)'`; \
	 set -x; \
	 $(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) r128.$(DRIVERMANSUFFIX).html $(DESTDIR)$(DOCHTMLDIR)/r128.$$SUF.html)

cleandir::
	$(RM) r128.$(DRIVERMANSUFFIX).html r128.$(DRIVERMANSUFFIX)-html

all:: r128.$(MANNEWSUFFIX)

r128.$(MANNEWSUFFIX)::  r128.$(MANSRCSUFFIX)
	$(RM) $@
	 	$(RAWCPP)  $(MANDEFS)  $(EXTRAMANDEFS) <r128.$(MANSRCSUFFIX) | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' 			-e '/^#line  *[0-9][0-9]*  *.*$$/d' 			-e '/^[ 	]*XCOMM$$/s/XCOMM/#/' 			-e '/^[ 	]*XCOMM[^a-zA-Z0-9_]/s/XCOMM/#/' 			-e '/^[ 	]*XHASH/s/XHASH/#/' 			-e '/\@\@$$/s/\@\@$$/\\/' >$@

cleandir::
	$(RM) r128.$(MANNEWSUFFIX)

install.man:: r128.$(MANNEWSUFFIX)
	@if [ -d $(DESTDIR)$(DRIVERMANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(DRIVERMANDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(DRIVERMANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(DRIVERMANDIR)); \
	fi
	$(INSTALL) -c $(INSTMANFLAGS) r128.$(MANNEWSUFFIX) \
		$(DESTDIR)$(DRIVERMANDIR)/r128.$(DRIVERMANSUFFIX)

